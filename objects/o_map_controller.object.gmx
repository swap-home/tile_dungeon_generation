            <string>if (global.map_ctrl &gt; 0) {show_error("Multiple map controllers detected! Aborting!", true);}
global.map_ctrl = id;

w = room_width/64;
h = room_height/64;
tile_types = ds_grid_create(w,h);
tile_ids = ds_grid_create(w,h);
tile_obj = ds_grid_create(w,h);

for (tile_x = 0; tile_x &lt; w; tile_x++) {
    for (tile_y = 0; tile_y &lt; h; tile_y++) {
        tile_types[# tile_x, tile_y] = 0;
    }
}

// Tile types 
//
//  0 = Unknown (Black), Unpathable
//  1 = Path (Grey)
//  2 = Sewer (Green), 2x move cost
//  3 = Stairs (Black - Grey Stairs) Start
//  4 = Stairs (Black - Grey Stairs) End


for (tile_x = 0; tile_x &lt; w; tile_x++) {
    for (tile_y = 0; tile_y &lt; h; tile_y++) {
        tile_ids[# tile_x, tile_y] = tile_add(testbg, 0, 0, 64, 64, tile_x*64, tile_y*64, 9001);
    }
}

tile_x = floor(w/2);
tile_y = floor(h/2);
tiles_total = w * h;
tiles_changed = 0;
last_choice = -1;
border_count = 0;
generated = false;

// set starting 2x2 to be Path (allow spawn room for 4 heros)
setTile(tile_x, tile_y, 1);
setTile(tile_x+1, tile_y, 1);
setTile(tile_x, tile_y+1, 1);
setTile(tile_x+1, tile_y+1, 1);

